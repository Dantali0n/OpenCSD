    "version": "0.2.0",
    "configurations": [
        {
            "name": "Local attach play-spdk",
            "type": "gdb",
            "request": "attach",
            "executable": "${workspaceFolder}/build/playground/play-spdk",
            "target": "localhost:2222",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "autorun": [
                "set sysroot ${workspaceRoot}"
            ]
        },
        {
            "name": "SSH attach play-spdk",
            "type": "gdb",
            "request": "launch",
            "target": "./build/playground/play-spdk",
            "cwd": "${workspaceRoot}",
            "ssh": {
                "host": "127.0.0.1",
                "port": 7777,
                "cwd": "/home/arch/qemu-csd/",
                "user": "root",
                "password": "arch",
                "bootstrap": "source /home/arch/qemu-csd/build/qemu-csd/activate",
                "forwardX11": false
            }
        },
        {
            "name": "SSH attach play-spdk-find-fill-filter",
            "type": "gdb",
            "request": "launch",
            "target": "./build/playground/play-spdk-find-fill-filter",
            "cwd": "${workspaceRoot}",
            "ssh": {
                "host": "127.0.0.1",
                "port": 7777,
                "cwd": "/home/arch/qemu-csd/",
                "user": "root",
                "password": "arch",
                "bootstrap": "source /home/arch/qemu-csd/build/qemu-csd/activate",
                "forwardX11": false
            }
        },
        {
            "name": "SSH attach qemucsd",
            "type": "gdb",
            "request": "launch",
            "target": "./build/qemu-csd/qemucsd",
            "cwd": "${workspaceRoot}",
            "ssh": {
                "host": "127.0.0.1",
                "port": 7777,
                "cwd": "/home/arch/qemu-csd/",
                "user": "root",
                "password": "arch",
                "bootstrap": "source /home/arch/qemu-csd/build/qemu-csd/activate",
                "forwardX11": false
            }
        }
    ]
}